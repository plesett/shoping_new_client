
.loadfail-img {
	background-color: red;
}
.countCalculation {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.countCalculation>view {
	width: 60rpx;
	padding: 5px;
	border: 1px solid #EEEEEE;
	background-color: #FFFFFF;
}
.Cover {
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	opacity: .7;
	z-index: 5;
	background-color: #000000;
}
.text-to {
	-webkit-transition: all 3s;
	transition: all 3s;
	display: inline-block;
	-webkit-animation: Tada 6s both infinite linear 0s;
	        animation: Tada 6s both infinite linear 0s;
}
@-webkit-keyframes Tada {
0% {
		-webkit-transform: translateX(600rpx);
		        transform: translateX(600rpx);
}
100% {
		-webkit-transform: translateX(-350rpx);
		        transform: translateX(-350rpx);
}
}
@keyframes Tada {
0% {
		-webkit-transform: translateX(600rpx);
		        transform: translateX(600rpx);
}
100% {
		-webkit-transform: translateX(-350rpx);
		        transform: translateX(-350rpx);
}
}

