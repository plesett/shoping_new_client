
.released {
	margin: 40rpx;
	padding: 20rpx;
	border-radius: 5px;
	border: 1px solid #008cff;
	background-color: #FFFFFF !important;
}
.released_body_title {
	float: left;
	font-size: 17px;
	overflow: hidden;
	line-height: 1.5;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	width: 80%;
	margin-bottom: 20rpx;
}
.price_info {
	color: #AAAAAA;
	width: 100%;
	clear: both;
}
.right_text {
	width: 18%;
	float: right;
	font-size: 13px;
	line-height: 70px;
	height: 55px;
}
.fts10 {
	font-size: 10px;
	color: #AAAAAA;
}
.audit {
	color: #F0AD4E;
}
.failure{
	color: #D84242;
}
.released_bottom {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 14px;
	padding-top: 15rpx;
	margin-top: 15rpx;
	border-top: 1px solid #EEEEEE;
}
.deleteFa {
	color: #cdcdcd;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.delete {
	width: 16px;
	height: 16px;
	margin-right: 2rpx;
}
.delete>image {
	width: 100%;
	height: 100%;
}

